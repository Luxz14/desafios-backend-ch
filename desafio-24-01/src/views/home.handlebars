<h1>Productos:</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Nombre</th>
        <th>Descripcion</th>
        <th>Codigo</th>
        <th>Precio</th>
        <th>Stock</th>
        <th>Imagenes</th>
    </tr>

    {{#each products}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.code}}</td>
            <td>{{this.price}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.thumbnails}}</td>
        </tr>
    {{/each}}    
</table>